<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://spring.www.springframework.org/schema/context"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd
http://spring.www.springframework.org/schema/context
http://spring.www.springframework.org/schema/context/spring-context.xsd">

    <bean class="com.springcore.lifeCycle.XmlLifeCycle" name="xmlLC1" init-method="init" destroy-method="destroy">
        <property name="price" value="10.25"/>
    </bean>

    <bean class="com.springcore.lifeCycle.InterfaceLC" name="intLC">
        <property name="price" value="5.0"/>
    </bean>

</beans>